<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>tach.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>face</object-name>
    <object-name>needle</object-name>

    <object-name>vitre</object-name>
  </animation>

  <!-- working for Rembrandt -->
  <effect>
    <inherits-from>Aircraft/bleriot-XI/Models/Effects/Glass/glass</inherits-from>
    <object-name>vitre</object-name>
  </effect>
  
  <!-- working for Rembrandt -->
  <effect>
    <inherits-from>Effects/model-transparent</inherits-from>
    <object-name>vitre</object-name>
  </effect>
  
  <animation>
    <type>rotate</type>
    <object-name>needle</object-name>
    <property>engines/engine/rpm</property>
    <interpolation> 
      <entry><ind>    0 </ind><dep>   0 </dep></entry>
      <entry><ind>  100 </ind><dep>  22 </dep></entry>
      <entry><ind>  600 </ind><dep>  53 </dep></entry>
      <entry><ind>  800 </ind><dep>  90 </dep></entry>
      <entry><ind> 1000 </ind><dep> 130 </dep></entry>
      <entry><ind> 1200 </ind><dep> 175 </dep></entry>
      <entry><ind> 1400 </ind><dep> 221 </dep></entry>
      <entry><ind> 1600 </ind><dep> 263 </dep></entry>
      <entry><ind> 1800 </ind><dep> 300 </dep></entry>
      <entry><ind> 2000 </ind><dep> 336 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>
